  "movement_rules": {
    "Entry": [
      ["(?i)input(s|ed|ing)?", "User", "System"],
      ["(?i)scan(s|ned|ning)?", "User", "System"],
      ["(?i)measure(s|d|ing)?", "Sensor", "System"],
      ["(?i)capture(s|d|ing)?", "External", "System"],
      ["(?i)detect(s|ed|ing)?", "Sensor", "System"],
      ["(?i)send(s|ing|ed)?", "User", "System"],
      ["(?i)submit(s|ting|ted)?", "User", "System"],
      ["(?i)enter(s|ing|ed)?", "User", "System"],
      ["(?i)create(s|ing|d)?", "User", "System"],
      ["(?i)login|log in", "User", "System"],
      ["(?i)request(s|ed|ing)?", "User", "System"],
      ["(?i)upload(s|ed|ing)?", "User", "System"],
      ["(?i)import(s|ed|ing)?", "User", "System"],
      ["(?i)provide(s|d|ing)?", "User", "System"],
      ["(?i)select(s|ed|ing)?", "User", "System"],
      ["(?i)register(s|ed|ing)?", "User", "System"],
      ["(?i)book(s|ed|ing)?", "User", "System"],
      ["(?i)trigger(s|ed|ing)?", "Clock", "System"],
      ["(?i)receive(s|d|ing)?", "External", "System"],
      ["(?i)callback", "External", "System"],
      ["(?i)on receipt of|upon receiving|when .* signal", "External", "System"],
      ["(?i)detect(s|ed|ing)?", "External", "System"],
      ["(?i)press(es|ed|ing)?|push(es|ed|ing)?", "User", "System"],
      ["(?i)rising( )?edge|falling( )?edge|tick|pulse", "Clock", "System"]
    ],
    "Read": [
      ["(?i)retrieve(s|d|ing)?", "Storage", "System"],
      ["(?i)read(s|ing|ed)?", "Storage", "System"],
      ["(?i)load(s|ed|ing)?", "Storage", "System"],
      ["(?i)fetch(es|ed|ing)?", "Storage", "System"],
      ["(?i)get(s|ting)?", "Storage", "System"],
      ["(?i)query(s|ing|ed)?", "Storage", "System"],
      ["(?i)access(es|ed|ing)?", "Storage", "System"],
      ["(?i)find(s|ing|found)?", "Storage", "System"],
      ["(?i)search(es|ed|ing)?", "Storage", "System"],
