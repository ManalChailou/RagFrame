  "forbidden_patterns": {
    "direct_storage_user": {
      "description": "Storage cannot communicate directly with User",
      "pattern": ["Storage", "User"],
      "decomposition": [
        ["Read", "Storage", "System"],
        ["Exit", "System", "User"]
      ]
    },
    "direct_user_storage": {
      "description": "User cannot communicate directly with Storage",
      "pattern": ["User", "Storage"],
      "decomposition": [
        ["Entry", "User", "System"],
        ["Write", "System", "Storage"]
      ]
    },
    "internal_system": {
      "description": "Internal System movements should be excluded from CFP",
      "pattern": ["System", "System"],
      "action": "exclude"
    },
    "external_to_external": {
      "description": "External entities cannot communicate directly",
      "pattern": ["External", "External"],
      "action": "exclude"
    },
    "storage_to_external": {
      "description": "Storage cannot communicate directly with External",
      "pattern": ["Storage", "External"],
      "action": "exclude"
    },
    "external_to_storage": {
      "description": "External devices (sensors, robots) should not write directly to Storage",
      "pattern": ["External", "Storage"],
      "action": "exclude"
    }
  },
  "validation_rules": {
    "mandatory_entry_per_process": true,
    "recommended_exit_per_process": true,
    "exclude_internal_movements": true,
    "enforce_layered_architecture": true,
    "allow_no_exit_for_realtime": true,
    "allow_external_integrations": true
  }
}